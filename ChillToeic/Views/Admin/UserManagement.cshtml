@{

    ViewBag.Title = "";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@model IEnumerable<ChillToeic.Models.Entity.User>

<div class="card-body">
    <p class="card-title mb-0">List of all learners</p>
    <div class="table-responsive">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Full name</th>
                    <th>Date of birth</th>
                    <th>Email</th>
                    <th>Point</th>
                    <th>Created at</th>
                    <th>RoleId</th>
                    <th>View</th>
                </tr>
            </thead>
            <tbody>
                @foreach(var user in Model){
                <tr>
                        <td>@Html.DisplayFor(modelItem => user.Id)</td>
                        <td>@Html.DisplayFor(modelItem => user.FullName)</td>
                        <td>@Html.DisplayFor(modelItem => user.DateOfBirth)</td>
                        <td>@Html.DisplayFor(modelItem => user.Email)</td>
                        <td>@Html.DisplayFor(modelItem => user.Point)</td>
                        <td>@Html.DisplayFor(modelItem => user.CreatedAt)</td>
                        <td>@Html.DisplayFor(modelItem => user.RoleId)</td>
                        <td><a class="btn1" href="/Admin/UserDetailManagement?id=@Html.DisplayFor(modelItem => user.Id)">View</a></td>
                </tr>
                }<!-- Thêm các hàng khác tương tự tại đây -->
            </tbody>

        </table>
    </div>
</div>
